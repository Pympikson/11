AACN,WAAK,KAAL,GAAa,KAAb;AACA,WAAK,QAAL,GAAgB,KAAhB;AACA,WAAK,UAAL,GAAkB,EAAlB;AACA,WAAK,QAAL,GAAgB,CAAhB;AACD;AAjHM,GA/BI;AAkJb,EAAA,KAAK,EAAE;AACL,IAAA,UADK,wBACQ;AACX,MAAA,YAAY,CAAC,OAAb,CAAqB,YAArB,EAAmC,KAAK,UAAxC;AACD;AAHI,GAlJM;AAuJb,EAAA,OAvJa,qBAuJH;AACR,IAAA,QAAQ,CAAC,gBAAT,CAA0B,SAA1B,EAAqC,KAAK,WAA1C;AACD,GAzJY;AA0Jb,EAAA,OA1Ja,qBA0JH;AACR,QAAI,IAAI,CAAC,KAAL,CAAW,YAAY,CAAC,OAAb,CAAqB,MAArB,CAAX,EAAyC,IAAzC,CAA8C,MAA9C,GAAuD,CAA3D,EACE,KAAK,KAAL,CAAW,IAAX,CAAgB,IAAhB,GAAuB,IAAI,CAAC,KAAL,CAAW,YAAY,CAAC,OAAb,CAAqB,MAArB,CAAX,EAAyC,IAAhE;AACF,QAAI,IAAI,CAAC,KAAL,CAAW,YAAY,CAAC,OAAb,CAAqB,UAArB,CAAX,EAA6C,IAA7C,CAAkD,MAAlD,GAA2D,CAA/D,EACE,KAAK,KAAL,CAAW,QAAX,CAAoB,IAApB,GAA2B,IAAI,CAAC,KAAL,CACzB,YAAY,CAAC,OAAb,CAAqB,UAArB,CADyB,EAEzB,IAFF;AAGF,QAAI,IAAI,CAAC,KAAL,CAAW,YAAY,CAAC,OAAb,CAAqB,WAArB,CAAX,EAA8C,IAA9C,CAAmD,MAAnD,GAA4D,CAAhE,EACE,KAAK,KAAL,CAAW,SAAX,CAAqB,IAArB,GAA4B,IAAI,CAAC,KAAL,CAC1B,YAAY,CAAC,OAAb,CAAqB,WAArB,CAD0B,EAE1B,IAFF;AAGH;AArKY,CAAf","sourcesContent":["<template>\r\n  <main class=\"main\">\r\n    <v-popup v-if=\"modal\" v-on:close=\"close\">\r\n      <div class=\"popup-form\">\r\n        <button @click=\"close\" class=\"popup-close\">\r\n          x\r\n        </button>\r\n        <label for=\"description\">Описание</label>\r\n        <input\r\n          type=\"text\"\r\n          v-model=\"modalTitle\"\r\n          name=\"description\"\r\n          class=\"popup-field\"\r\n        />\r\n        <label for=\"lvl\">Приоритет</label>\r\n        <select name=\"lvl\" class=\"popup-field\" v-model=\"modalLvl\">\r\n          <option value=\"1\" class=\"popup-select\">1</option>\r\n          <option value=\"2\" class=\"popup-select\">2</option>\r\n          <option value=\"3\" class=\"popup-select\">3</option>\r\n        </select>\r\n        <button\r\n          @click=\"save(tasks.plan.todo)\"\r\n          v-if=\"!editBool\"\r\n          class=\"popup-save\"\r\n        >\r\n          Сохранить\r\n        </button>\r\n        <button v-else @click=\"saveEdit\" class=\"popup-save\">\r\n          Внести изменения\r\n        </button>\r\n      </div>\r\n    </v-popup>\r\n    <div class=\"container\">\r\n      <button\r\n        @click=\"\r\n          modal = true;\r\n          modalTitle = '';\r\n          modalLvl = 1;\r\n        \"\r\n        class=\"main-btn\"\r\n      >\r\n        Создать задачу\r\n      </button>\r\n      <div class=\"main-wrapper\">\r\n        <v-plan\r\n          v-for=\"(item, key) in tasks\"\r\n          :key=\"key\"\r\n          :todoArr=\"item\"\r\n          v-on:inLeft=\"inLeft\"\r\n          v-on:inRight=\"inRight\"\r\n          v-on:editTask=\"editTask\"\r\n          v-on:onDrop=\"onDrop\"\r\n        />\r\n      </div>\r\n    </div>\r\n  </main>\r\n</template>\r\n\r\n<script>\r\nimport vPlan from \"./vPlan\";\r\nimport vPopup from \"./vPopup\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      modal: false,\r\n      modalid: null,\r\n      modalTitle: \"\",\r\n      editBool: false,\r\n      editTitle: null,\r\n      left: null,\r\n      modalLvl: 1,\r\n      countTasks: +localStorage.getItem(\"countTasks\") || 0,\r\n      tasks: {\r\n        plan: {\r\n          title: \"План\",\r\n          todo: [],\r\n        },\r\n        progress: {\r\n          title: \"В работе\",\r\n          todo: [],\r\n        },\r\n        complited: {\r\n          title: \"Готово\",\r\n          todo: [],\r\n        },\r\n      },\r\n    };\r\n  },\r\n  components: {\r\n    vPlan,\r\n    vPopup,\r\n  },\r\n  methods: {\r\n    onDrop(value) {\r\n      this.tasks[value.where].todo.push(this.tasks[value.from].todo[value.id]);\r\n      this.tasks[value.from].todo.splice(value.id, 1);\r\n      localStorage.setItem(\r\n        `${value.where}`,\r\n        JSON.stringify(this.tasks[value.where])\r\n      );\r\n      localStorage.setItem(\r\n        `${value.from}`,\r\n        JSON.stringify(this.tasks[value.from])\r\n      );\r\n    },\r\n    saveEdit() {\r\n      switch (this.editTitle) {\r\n        case \"План\":\r\n          this.tasks.plan.todo.forEach((item) => {\r\n            if (item.id === this.modalid) {\r\n              item.description = this.modalTitle;\r\n              item.lvl = this.modalLvl;\r\n              return;\r\n            }\r\n          });\r\n          this.modal = false;\r\n          localStorage.plan = JSON.stringify(this.tasks.plan);\r\n          break;\r\n        case \"В работе\":\r\n          this.tasks.progress.todo.forEach((item) => {\r\n            if (item.id === this.modalid) {\r\n              item.description = this.modalTitle;\r\n              item.lvl = this.modalLvl;\r\n              return;\r\n            }\r\n          });\r\n          this.modal = false;\r\n          localStorage.setItem(\"progress\", JSON.stringify(this.tasks.progress));\r\n          break;\r\n        case \"Готово\":\r\n          this.tasks.complited.todo.forEach((item) => {\r\n            if (item.id === this.modalid) {\r\n              item.description = this.modalTitle;\r\n              item.lvl = this.modalLvl;\r\n              return;\r\n            }\r\n          });\r\n          this.modal = false;\r\n          localStorage.setItem(\r\n            \"complited\",\r\n            JSON.stringify(this.tasks.complited)\r\n          );\r\n          break;\r\n      }\r\n    },\r\n    onKeyEscape(e) {\r\n      if (e.key === \"Escape\") {\r\n        this.close();\r\n      }\r\n    },\r\n    editTask(value) {\r\n      this.modalLvl = value.task.lvl;\r\n      this.modalTitle = value.task.description;\r\n      this.editBool = true;\r\n      this.modal = true;\r\n      this.editTitle = value.title;\r\n      this.modalid = value.id;\r\n    },\r\n    inLeft(value) {\r\n      if (value.title === \"Готово\") {\r\n        this.tasks.progress.todo.push(this.tasks.complited.todo[value.id]);\r\n        this.tasks.complited.todo.splice(value.id, 1);\r\n        localStorage.setItem(\"complited\", JSON.stringify(this.tasks.complited));\r\n      } else {\r\n        this.tasks.plan.todo.push(this.tasks.progress.todo[value.id]);\r\n        this.tasks.progress.todo.splice(value.id, 1);\r\n        localStorage.plan = JSON.stringify(this.tasks.plan);\r\n      }\r\n      localStorage.setItem(\"progress\", JSON.stringify(this.tasks.progress));\r\n    },\r\n    inRight(value) {\r\n      console.log(this.tasks);\r\n      if (value.title === \"План\") {\r\n        this.tasks.progress.todo.push(this.tasks.plan.todo[value.id]);\r\n        this.tasks.plan.todo.splice(value.id, 1);\r\n        localStorage.plan = JSON.stringify(this.tasks.plan);\r\n      } else {\r\n        this.tasks.complited.todo.push(this.tasks.progress.todo[value.id]);\r\n        this.tasks.progress.todo.splice(value.id, 1);\r\n        localStorage.setItem(\"complited\", JSON.stringify(this.tasks.complited));\r\n      }\r\n      localStorage.setItem(\"progress\", JSON.stringify(this.tasks.progress));\r\n    },\r\n    save(arr) {\r\n      this.countTasks += 1;\r\n      const newTodo = {\r\n        id: this.countTasks,\r\n        lvl: this.modalLvl,\r\n        description: this.modalTitle,\r\n        date: new Date(),\r\n      };\r\n      arr.push(newTodo);\r\n      localStorage.setItem(\r\n        \"plan\",\r\n        JSON.stringify({ title: \"План\", todo: arr })\r\n      );\r\n      this.modalLvl = 1;\r\n      this.modalTitle = \"\";\r\n      this.modal = false;\r\n    },\r\n    close() {\r\n      this.modal = false;\r\n      this.editBool = false;\r\n      this.modalTitle = \"\";\r\n      this.modalLvl = 1;\r\n    },\r\n  },\r\n  watch: {\r\n    countTasks() {\r\n      localStorage.setItem(\"countTasks\", this.countTasks);\r\n    },\r\n  },\r\n  created() {\r\n    document.addEventListener(\"keydown\", this.onKeyEscape);\r\n  },\r\n  mounted() {\r\n    if (JSON.parse(localStorage.getItem(\"plan\")).todo.length > 0)\r\n      this.tasks.plan.todo = JSON.parse(localStorage.getItem(\"plan\")).todo;\r\n    if (JSON.parse(localStorage.getItem(\"progress\")).todo.length > 0)\r\n      this.tasks.progress.todo = JSON.parse(\r\n        localStorage.getItem(\"progress\")\r\n      ).todo;\r\n    if (JSON.parse(localStorage.getItem(\"complited\")).todo.length > 0)\r\n      this.tasks.complited.todo = JSON.parse(\r\n        localStorage.getItem(\"complited\")\r\n      ).todo;\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.popup {\r\n  &-close {\r\n    height: 30px;\r\n    width: 30px;\r\n    border-radius: 50%;\r\n    align-self: flex-end;\r\n  }\r\n  &-field,\r\n  &-select {\r\n    color: var(--text-input);\r\n    background-color: var(--bg-input);\r\n    border-radius: 15px;\r\n    padding: 5px;\r\n    outline: none;\r\n  }\r\n  &-form {\r\n    padding: 15px 30px;\r\n    color: var(--text-modal);\r\n    display: flex;\r\n    flex-direction: column;\r\n    width: 400px;\r\n  }\r\n}\r\n.main {\r\n  flex-grow: 1;\r\n  background-color: var(--bg-main);\r\n  color: var(--text-color);\r\n  padding-top: 40px;\r\n  &-wrapper {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    flex-wrap: wrap;\r\n  }\r\n  &-btn {\r\n    padding: 10px 5px;\r\n    margin-bottom: 20px;\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}